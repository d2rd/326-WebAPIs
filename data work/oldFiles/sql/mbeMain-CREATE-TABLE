-- DROP TABLE "mdDOT"."mbeMain";

CREATE TABLE "mdDOT"."mbeMain"
(
    address text COLLATE pg_catalog."default",
    city text COLLATE pg_catalog."default",
    contact text COLLATE pg_catalog."default",
    county text COLLATE pg_catalog."default",
    "idmbeMain" integer NOT NULL,
    minority_group text COLLATE pg_catalog."default",
    name text COLLATE pg_catalog."default" NOT NULL,
    state text COLLATE pg_catalog."default",
    zip text COLLATE pg_catalog."default",
    phone_type text COLLATE pg_catalog."default",
    phone text COLLATE pg_catalog."default",
    fax_type text COLLATE pg_catalog."default",
    certification_number text COLLATE pg_catalog."default",
    fax text COLLATE pg_catalog."default",
    contact_email_address text COLLATE pg_catalog."default",
    certification_date time without time zone,
    website_description text COLLATE pg_catalog."default",
    website text COLLATE pg_catalog."default",
    naics_code text COLLATE pg_catalog."default",
    products_services text COLLATE pg_catalog."default",
    geocoded_location_address text COLLATE pg_catalog."default",
    geocoded_location_state text COLLATE pg_catalog."default",
    geocoded_location_zip text COLLATE pg_catalog."default",
    geocoded_location_city text COLLATE pg_catalog."default",
    geocoded_location point,
    "certificationType" text COLLATE pg_catalog."default",
    CONSTRAINT "mbeMain_pkey" PRIMARY KEY ("idmbeMain")
)
WITH (
    OIDS = FALSE
)
TABLESPACE pg_default;

